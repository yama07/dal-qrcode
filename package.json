  "devDependencies": {
    "@iconify-json/mdi": "1.2.3",
    "@internationalized/date": "3.9.0",
    "@lucide/svelte": "0.544.0",
    "@playwright/test": "1.55.1",
    "@tsconfig/svelte": "5.0.5",
    "@types/node": "24.5.2",
    "@types/qrcode": "1.5.5",
    "@wxt-dev/auto-icons": "1.1.0",
    "@wxt-dev/module-svelte": "2.0.4",
    "bits-ui": "2.11.4",
    "clsx": "2.1.1",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.39.6",

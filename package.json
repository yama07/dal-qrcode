{
  "name": "dal-qrcode",
  "description": "A simple QR code generator/scanner üêæ",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
